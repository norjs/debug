{
  "name": "@norjs/debug",
  "version": "1.3.2",
  "description": "Debug helpers for JavaScript",
  "main": "dist/debug.js",
  "scripts": {
    "compile-watch": "babel --watch --presets es2015 -d dist/ src/",
    "compile": "babel --presets es2015 -d dist/ src/",
    "prepare": "npm run -s compile",
    "pretest": "npm run -s compile",
    "test": "mocha --require babel-register test/unit/**/*.spec.js",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "lint": "node-lint src/ --config=lint.json",
    "jshint": "jshint src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/norjs/debug"
  },
  "keywords": [
    "debug",
    "assert"
  ],
  "dependencies": {
    "@norjs/is": "^1.3.2",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "jshint": "^2.9.7",
    "lint": "^1.1.2",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1"
  },
  "author": "Jaakko-Heikki Heusala <jheusala@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/norjs/debug/issues"
  }
}
